{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "linebreak-style": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-restricted-globals": "off",
      "import/extensions": "off",
      "import/prefer-default-export": "off"
    }
}
